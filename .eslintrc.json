{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "import/prefer-default-export": 0,
        "max-len": [ "error", 
            { 
                "code": 120,
                "ignoreStrings": true,
                "ignoreTemplateLiterals": true,
                "ignoreRegExpLiterals": true
               
            } 
        ],
        "jsx-a11y/anchor-is-valid": 0
    }
}